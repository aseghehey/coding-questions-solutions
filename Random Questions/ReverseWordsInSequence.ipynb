    "s = 'Hello today is a good day'\n",
    "def reverseWordsInSequence(strng):\n",
    "    sArr = strng.split()\n",
    "    for i in range(len(sArr)):\n",
    "        sArr[i] = sArr[i][::-1]\n",
    "    return ' '.join(sArr)\n",
    "\n",
    "print(reverseWordsInSequence(s))"
   ]
