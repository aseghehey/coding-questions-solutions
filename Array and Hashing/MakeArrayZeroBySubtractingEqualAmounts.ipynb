    "def minimumOperations(self, nums: List[int]) -> int:\n",
    "    count = 0\n",
    "    d = Counter(nums)\n",
    "    for k in d.keys():\n",
    "        if k > 0:\n",
    "            count +=1\n",
    "    return count"
   ]
