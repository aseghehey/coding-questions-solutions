    "def canJump(arr):\n",
    "    goal = len(arr) - 1\n",
    "    for i in range(len(arr) - 1, -1, -1):\n",
    "        if i + arr[i] >= goal:\n",
    "            goal = i\n",
    "    return goal == 0"
   ]
